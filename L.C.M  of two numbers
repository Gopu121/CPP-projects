#include <iostream>
using namespace std;

int main() {
    int a, b, i;

    cout << "Enter two numbers: ";
    cin >> a >> b;

    for (i = 2; i <= a * b; ++i) {
        if (i % a == 0 && i % b == 0)
            break;
    }

    cout << "LCM of the numbers " << a << " and " << b << " is " << i << endl;

    return 0;
}
